  "keywords": [
    "react",
    "sendbird"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sendbird/sendbird-uikit-react-native.git",
    "directory": "packages/uikit-react-core"
  },
  "author": "Sendbird <support@sendbird.com>",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/sendbird/sendbird-uikit-react-native.git/issues"
  },
  "homepage": "https://github.com/sendbird/sendbird-uikit-react-native.git#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@sendbird/chat-react-hooks": "^0.1.0"
  },
  "devDependencies": {
    "@react-native-async-storage/async-storage": "^1.15.16",
    "@types/react": "^16.9.19",
    "@types/react-native": "^0.63.x",
    "react": "^16.13.1",
    "react-native": "^0.63.3",
    "react-native-builder-bob": "^0.18.0",
    "react-native-image-picker": "^4.7.1",
    "react-native-permissions": "^3.2.0",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": ">=1.15.16",
    "react": ">=16.13.1",
    "react-native": ">=0.63.3",
    "react-native-image-picker": "^4.7.1",
    "react-native-permissions": "^3.2.0"
  },
  "peerDependenciesMeta": {
    "react-native-image-picker": {
      "optional": true
    },
    "react-native-permissions": {
      "optional": true
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
