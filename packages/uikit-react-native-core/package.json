{
  "name": "@sendbird/uikit-react-native-core",
  "version": "0.1.0",
  "description": "SendbirdUIKit Core utilities for building UIKit",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
